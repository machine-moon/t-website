    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.2.0",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "prettier": "^3.6.2",
    "vite": "^7.0.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/t-ibrahimm/t-ibrahimm.github.io.git"
